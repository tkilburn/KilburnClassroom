{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "node-sass": "^4.5.3",
    "node-sass-chokidar": "0.0.1",
    "nodemon": "^1.11.0",
    "react-scripts": "1.0.3"
  },
  "scripts": {
    "build-css": "node-sass --include-path scss src/css/scss/main.scss src/css/bundle.css --output-style compressed",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "zzz_eject": "react-scripts eject",
    "watch": "concurrently --names \"webpack,node-scss\" --prefix name \"npm run start\" \"npm run watch-css\""
  }
}
